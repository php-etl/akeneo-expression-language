{
    "name": "php-etl/akeneo-expression-language",
    "description": "This library implements functions for manupulating Akeneo API data in ExpressionLanguage",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "nikic/php-parser": "^4.10",
        "symfony/expression-language": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "johnkary/phpunit-speedtrap": "4.0.x-dev",
        "mybuilder/phpunit-accelerator": "*",
        "adlawson/vfs": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\ExpressionLanguage\\Akeneo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Kiboko\\Component\\ExpressionLanguage\\Akeneo\\": "spec/",
            "unit\\Kiboko\\Component\\ExpressionLanguage\\Akeneo\\": "tests/unit/",
            "functional\\Kiboko\\Component\\ExpressionLanguage\\Akeneo\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
